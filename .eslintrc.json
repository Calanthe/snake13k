{
    "env": {
        "browser": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "indent": ["error", "tab", { "SwitchCase": 1 }],
      "no-fallthrough": "off",
      "semi": [ "error", "always" ],
      "no-console": "off",
      "no-sparse-arrays": "off",
      "eqeqeq": "error"
    }
}
